<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : <%= ENV['APP_ID']%>,
      xfbml      : true,
      version    : 'v2.6'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>


<form accept-charset="UTF-8" action="/welcome/index" method="post">
  <input type="text" value= <%= @customerNumber %> name="customerNumber" id="customerNumber"/>
  <input name="commit" type="submit" value="Change Customer Number" />
</form>
